{
  "expo": {
    "name": "Endur_Track",
    "slug": "Endur_Track",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "platforms": [
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "enableProguardInReleaseBuilds": true,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.arjunk2004.Endur_Track"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8a5a17fd-6f8c-4fdc-aab3-f182e3828a4d"
      }
    },
    "owner": "arjunk2004"
  }
}
